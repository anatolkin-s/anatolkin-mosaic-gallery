{
  "name": "anatolkin/mosaic-gallery",
  "description": "Anatolkin Mosaic Gallery: masonry-like image gallery for TYPO3 CMS 13 using FAL and optional GLightbox.",
  "type": "typo3-cms-extension",
  "version": "0.1.6",
  "license": "MIT",
  "keywords": [
    "typo3",
    "gallery",
    "masonry",
    "images",
    "fal",
    "lightbox",
    "glightbox"
  ],
  "authors": [
    {
      "name": "Sergey Fedorov",
      "email": "typo3@anatolkin.com"
    }
  ],
  "require": {
    "php": ">=8.1 <8.4",
    "typo3/cms-core": "^13.4"
  },
  "autoload": {
    "psr-4": {
      "Anatolkin\\MosaicGallery\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "anatolkin_mosaic_gallery"
    }
  },
  "support": {
    "issues": "https://github.com/anatolkin-s/mosaic_gallery/issues",
    "source": "https://github.com/anatolkin-s/mosaic_gallery"
  }
}

